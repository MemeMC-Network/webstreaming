{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/socket.io/(.*)",
      "destination": "/api/socketio"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
